<%- include ("userheader.ejs") %>

<%- include ("usernav.ejs") %>

            <!-- About Start -->
            <div class="about wow fadeInUp" data-wow-delay="0.1s">
                <div class="container">
                    <div class="row align-items-center">
                        <div class="col-lg-12 col-md-12">
                            <div class="about-text">
<h2>Add Rental Property Here!!!</h2>
<font style="color:blue;"><%- output %></font>
<form method="post" action="/admin/addcategory" enctype="multipart/form-data">
    <div class="form-group">
      <label for="title">Title:</label>
      <input type="text" class="form-control" id="title" name="title">
    </div>
    <div class="form-group">
      <label for="catname">SubCategory Name:</label>
      <select class="form-control" id="subcatname" name="subcatname">
        <option>Select Sub Category</option>
        <% for(let row of sclist) { %>
          <option><%- row.subcatname %></option>
        <% } %>
      </select>
    </div>
    <div class="form-group">
        <label for="pdescription">Property Description:</label>
        <textarea class="form-control" id="pdescription" name="pdescription"></textarea>
    </div>
    <div class="form-group">
      <label for="picon">Property Icon:</label>
      <input type="file" class="form-control" id="picon" name="picon"> 
    </div>
    <button type="submit" class="btn btn-success">Add Property</button>
  </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- About End -->

<%- include ("userfooter.ejs") %>            


